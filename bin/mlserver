#!/usr/bin/env ruby

require "MLserver"
require "argparse"

MLserver.settings = MLserver::Settings.new()

o = {}
s = {}

x = ArgsParser::Args.new(options: o, switches: s)

conf_file = x.data[0]

puts File.expand_path(conf_file, __FILE__)

require File.expand_path(conf_file, Dir.pwd)

MLserver::Server.start()
